  "source": "pragma solidity ^0.4.4;\n\ncontract Sale {\n  address public seller;\n  address public buyer;\n  uint public value;\n  string public name;\n\n  modifier onlySeller() {\n    if (msg.sender == seller)\n    _;\n  }\n\n  modifier onlyBuyer() {\n    if (msg.sender == buyer)\n    _;\n  }\n\n  function Sale(string _name) payable public {\n    seller = msg.sender;\n    value = msg.value;\n    name = _name;\n  }\n\n  function willBuy() public {\n    buyer = msg.sender;\n  }\n}",
